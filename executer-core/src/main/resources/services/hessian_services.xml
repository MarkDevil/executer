<?xml version="1.0" encoding="UTF-8"?>
<services>

	<!--账务系统-->
	<hessian id="accountIn"
			 url="${baseAccountingUrl}/accountingFacade"
			 interface="cn.creditease.pay.account.api.service.AccountingFacade"
			 method="accountIn"
			 desc="交易系统-单次入账"
	/>

	<hessian id="transfer"
			 url="${baseAccountingUrl}/accountingFacade"
			 interface="cn.creditease.pay.account.api.service.AccountingFacade"
			 method="transfer"
			 desc="交易系统-转账"
	/>

	<hessian id="batchAccountIn"
			 url="${baseAccountingUrl}/accountingFacade"
			 interface="cn.creditease.pay.account.api.service.AccountingFacade"
			 method="batchAccountIn"
			 desc="交易系统-批量入账"
	/>

</services>
